<%- include("header") -%>

</header>
<div class="box" id="heading">
    <h1><%= listTitle %></h1>
</div>
<div class="box" >
  <% for(var i =0; i<newit.length; i++){ %>
  <div class="item">
    <input type="checkbox" >
      <p><%= newit[i] %></p>
</div>
  <%}%>
  <form class="item" action="/" method="post">
    <input type="text" name="new" placeholder="Add new item" autocomplete="off">
    <button type="submit" name="list" value=<%=listTitle%> >+</button>
  </form>
</div>
<%- include("footer") -%>
